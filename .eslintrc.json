{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-unused-vars": "warn",
    "no-underscore-dangle": "warn",
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ]
  }
}
